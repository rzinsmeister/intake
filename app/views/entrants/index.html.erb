<h1>Children</h1>

<div id="indexbtn" class="btn">
<%= link_to 'New Entrant', new_entrant_path %>
</div>


<div class="span11 entrants-wrapper">

<table class="table table-condensed table-hover">
  <tr>
    <th>Child's First Name</th>
    <th>Child's Last Name</th>
    <th>Contactid</th>
    <th>Date of Birth</th>
    <th>Grade</th>
    <th>Shelterid</th>
    <th>Mnres</th>
    <th>Entrydate</th>
    <th>Exitdate</th>
    <th>Totaldays</th>
    <th>Lastschool</th>
    <th>Shelterschool</th>
    <th>Mpsschool</th>
    <th>Nonmpsschool</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @entrants.each do |entrant| %>
  <tr>
    <td><%= entrant.cnamef %></td>
    <td><%= entrant.cnamel %></td>
    <td><%= entrant.contact_id %></td>
    <td><%= entrant.dob %></td>
    <td><%= entrant.grade %></td>
    <td><%= entrant.shelterid %></td>
    <td><%= entrant.mnres %></td>
    <td><%= entrant.entrydate %></td>
    <td><%= entrant.exitdate %></td>
    <td><%= entrant.totaldays %></td>
    <td><%= entrant.lastschool %></td>
    <td><%= entrant.shelterschool %></td>
    <td><%= entrant.mpsschool %></td>
    <td><%= entrant.nonmpsschool %></td>
    <td><%= entrant.note %></td>
    <td><%= link_to 'Show', entrant %></td>
    <td><%= link_to 'Edit', edit_entrant_path(entrant) %></td>
    <td><%= link_to 'Destroy', entrant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
</div>

<br />

