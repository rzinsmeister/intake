<h1>Listing contacts</h1>

<table>
  <tr>
    <th>Pnamef</th>
    <th>Pnamel</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Phone</th>
    <th>Entrydate</th>
    <th>Exitdate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.pnamef %></td>
    <td><%= contact.pnamel %></td>
    <td><%= contact.street %></td>
    <td><%= contact.city %></td>
    <td><%= contact.state %></td>
    <td><%= contact.zip %></td>
    <td><%= contact.phone %></td>
    <td><%= contact.entrydate %></td>
    <td><%= contact.exitdate %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact', new_contact_path %>
